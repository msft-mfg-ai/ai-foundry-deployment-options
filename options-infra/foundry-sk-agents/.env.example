# Environment variables for local development
# Copy this file to .env and fill in your values

# Azure Application Insights - Required for OpenTelemetry
APPLICATIONINSIGHTS_CONNECTION_STRING=

# Azure AI Foundry Project Connection String
# Format: <endpoint>;<subscription_id>;<resource_group>;<project_name>
AZURE_AI_PROJECT_CONNECTION_STRING=

# Alternative: Direct Azure OpenAI endpoint (if not using Foundry)
AZURE_OPENAI_ENDPOINT=
AZURE_OPENAI_DEPLOYMENT=gpt-4.1-mini

# Azure Identity (for local authentication)
# Use 'az login' for interactive authentication, or set these for service principal:
AZURE_CLIENT_ID=
AZURE_TENANT_ID=
AZURE_CLIENT_SECRET=

# File Processor Settings
PROCESSING_DELAY_SECONDS=180
